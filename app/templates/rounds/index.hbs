<h3>{{title}}</h3>
<hr>
<div class="row">
  <div class="btn-block">
      {{#link-to 'new-round'}}<button class="btn btn-default new-round-btn">New Round</button>{{/link-to}}
  </div> 
</div>

<div class="container">
  <ul class="nav nav-tabs">
      <li class="active"><a data-toggle="tab" href="#home">{{currentYear}}</a></li>
      {{#each months as |month|}}
          <li><a data-toggle="tab" href="#{{month}}">{{month}}</a></li>
      {{/each}}
  </ul>

  <div class="tab-content">
    <div id="home" class="tab-pane fade in active" style="margin-top: 50px;">
      {{year-stats model=model}}
    </div>
    {{#each months as |month|}}
      <div id="{{month}}" class="tab-pane fade">
          {{card-overview model=model month=month}}
      </div>
    {{/each}}
  </div>
</div>

